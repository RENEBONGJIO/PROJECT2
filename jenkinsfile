pipeline{

    agent any

    stages{

        stage('Continuous Download') {

            steps{
                git branch: 'main', url 'https://github.com/RENEBONGJIO/PROJECT2.git'
            }
        }
        stage('Unit Test'){

            steps{
                sh'mvn test'
            }
        }
        stage('Integration Test'){

            steps{
                sh'mvn verify -DskipUnitTests'
            }
        }
        stage('Continuous Build'){

            steps{
                sh'mvn clean install'
            }
        }
    }      
}      